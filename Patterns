 *   *
*     *
 *   *
  * *
   *

int main()
{
    int n;
    cout<<"Enter n: ";
    cin>>n;
    for(int i=0;i<n;i++)
    {
        for(int j=n;j>i;j--)
        {
            cout<<" ";
        }
        for(int j=0;j<i;j++)
        {
            if(j==0)
            {
                cout<<"*";
            }
            else{
                cout<<" ";
            }
        }
        for(int j=1;j<i;j++)
        {
            if(j==i-1)
            {
                cout<<"*";
            }
            else
            {
                cout<<" ";
            }
        }
        cout<<endl;
    }
    for(int i=n;i>0;i--)
    {
        for(int j=n;j>i;j--)
        {
            cout<<" ";
        }
        for(int j=0;j<i;j++)
        {
            if(j==0)
            {
                cout<<"*";
            }
            else{
                cout<<" ";
            }
        }
        for(int j=1;j<i;j++)
        {
            if(j==i-1)
            {
                cout<<"*";
            }
            else
            {
                cout<<" ";
            }
        }
        cout<<endl;
    }
}

*      *
**    **
***  ***
********
***  ***
**    **
*      *

int main()
{
    int n;
    cout<<"Enter n: ";
    cin>>n;
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<=i;j++)
        {
            cout<<"*";
        }
        for(int j=n;j>i+1;j--)
        {
            cout<<" ";
        }
        for(int j=n;j>i+1;j--)
        {
            cout<<" ";
        }
        for(int j=0;j<=i;j++)
        {
            cout<<"*";
        }
        cout<<endl;
    }
    for(int i=n-2;i>=0;i--)
    {
        for(int j=0;j<=i;j++)
        {
            cout<<"*";
        }
        for(int j=n;j>i+1;j--)
        {
            cout<<" ";
        }
        for(int j=n;j>i+1;j--)
        {
            cout<<" ";
        }
        for(int j=0;j<=i;j++)
        {
            cout<<"*";
        }
        cout<<endl;
    }
}
