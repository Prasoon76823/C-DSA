int main() {
    int n;
    cout<<"Enter n: ";
    cin>>n;
    for(int i=n;i>0;i--)
    {
        for(int j=0;j<i;j++)
        {
            cout<<i<<"\t";
        }
        cout<<endl;
    }
}

