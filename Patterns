int main() {
    int n;
    cout<<"Enter n: ";
    cin>>n;
    char ch='A';
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<i;j++)
        {
            cout<<" ";
        }
        for(int j=0;j<n-i;j++)
        {
            cout<<ch;
        }
        cout<<endl;
        ch++;
    }
}

