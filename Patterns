int main() {
    int n;
    cout<<"Enter n: ";
    cin>>n;
    char ch='A';
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<=i;j++)
        {
            cout<<ch<<"\t";
        }
        cout<<endl;
        ch++;
    }
}

