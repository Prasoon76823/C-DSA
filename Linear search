#include <iostream>
#include <climits>
using namespace std;
int main() {
    int n,a[100];
    cout<<"Enter size of array: ";
    cin>>n;
    cout<<"Enter array: ";
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    int ele,f=0;
    cout<<"Enter element to be searched: ";
    cin>>ele;
    for(int i=0;i<n;i++)
    {
        if(a[i]==ele)
        {
            f=1;
            cout<<"Element found!!  at "<<i;
            break;
        }
    }
    if(f==0)
    {
        cout<<"Not found!!";
    }
}
