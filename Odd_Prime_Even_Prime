#include<iostream>
using namespace std;
int main()
{
    cout<<"Odd Numbers: ";
    for(int i=1;i<=50;i++)
    {
        if(i%2!=0)
        {
            cout<<i<<" ";
        }
    }
    cout<<endl;
    cout<<"Prime Numbers: ";
    for(int i=1;i<=50;i++)
    {
        int count=0;
        for(int j=2;j<=i/2;j++){
            if(i%j==0)
            {
                count++;
                break;
            }
        }
        if(count==0)
        {
            cout<<i<<" ";
        }
    }
    cout<<endl;
    cout<<"Even Number: ";
    for(int i=50;i<=100;i++)
    {
        if(i%2==0)
        {
            cout<<i<<" ";
        }
    }
    cout<<endl;
    cout<<"Prime Numbers: ";
    for(int i=50;i<=100;i++)
    {
        int count=0;
        for(int j=2;j<=i/2;j++){
            if(i%j==0)
            {
                count++;
                break;
            }
        }
        if(count==0)
        {
            cout<<i<<" ";
        }
    }
}
